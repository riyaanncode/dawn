{%- case media.media_type -%}
    {%- when 'image' -%}
        <img src="{{ media | img_url: 'large' }}" class="imag_contenrt" width="" height="">
    {%- when 'external-video' -%}
        {{ media | external_video_tag: class: 'external_vido' }}
    {%- when 'video' -%}
        {{ media | video_tag: class: 'video-tag' }}
    {%- when 'model' -%}
        <!-- Add your model rendering code here if needed -->
    {%- else -%}
        <!-- Handle other cases or leave empty -->
    {%- endcase -%}
